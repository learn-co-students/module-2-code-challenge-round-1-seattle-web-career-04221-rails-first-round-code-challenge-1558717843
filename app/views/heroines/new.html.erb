
<%if flash[:errors] %>
That was not a valid entry because
<%flash[:errors].each do |e| %>
<%= e %><br>
<% end %>
Please try again.
<% end %>


<h1>Create A New Super Heroine!</h1>

<%=form_for @heroine do |f| %>
<%=f.label :name %> <%=f.text_field :name%>
<%=f.label :super_name %> <%=f.text_field :super_name %>
<%=f.label :power_id %> <%=f.collection_select(:power_id, Power.all, :id, :name)%>

<%=f.submit%>
<%end%>
