<h2>All Heroines</h2>

<ul>
  <label>Search By Superpower:</label>
  <%= form_tag(heroines_path, method: "get") do %>
  <%= text_field_tag(:power_name) %>
  <%= submit_tag("Search") %>
<% end %>
  <p></p>
  <%= link_to "Create Heroine", new_heroine_path%>
  <p></p>
  <% @heroines.each do |heroine| %>
    <li>
      <%= link_to heroine.super_name, heroine %>
    </li>
    <br>
  <% end %>
</ul>
